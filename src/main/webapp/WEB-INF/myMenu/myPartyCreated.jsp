				}
			}else{
				confirmYn = 'Y';
			}
		}else{
			confirmYn = 'Y';
		}
	}

	if(confirmYn == 'Y'){
		var ajax_url = "https://buts.co.kr/shop/partner/ajax.date_up.php";

		$.ajax({
			type: "POST",
			url: ajax_url,
			data: {
