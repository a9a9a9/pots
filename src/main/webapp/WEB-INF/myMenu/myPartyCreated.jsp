				'pt_level'   : pt_level
			},
			cache: false,
			async: false,
			dataType: "json",
			success: function(data, textStatus) {
					error = data.error;
			}
		});
		if (error) {
			alert(error);
			location.reload();
			return false;
		}		
	}
}	
