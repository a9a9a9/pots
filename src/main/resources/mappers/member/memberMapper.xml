<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.proj.pots.membership.dao.IMemberDAO">

	<select id="isExistId" parameterType="String" resultType="int">
		SELECT count(*) FROM login WHERE id=#{id}
	</select>
	
	<insert id="insertMember" parameterType="MemberDTO">
		INSERT INTO member VALUES(#{id}, #{nick}, #{pw}, #{name},  #{tel}, #{profile,jdbcType=VARCHAR}, #{point,jdbcType=VARCHAR}, #{joined_date,jdbcType=VARCHAR}, #{personal_num,jdbcType=VARCHAR},#{account_name,jdbcType=VARCHAR}, #{account_num,jdbcType=VARCHAR})
	</insert>
</mapper>